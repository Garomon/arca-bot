}

.due-label {
    font-size: 0.75rem;
    color: var(--text-muted);
}

.due-date {
    font-size: 0.85rem;
    font-weight: 600;
    color: var(--text-primary);
}

.debt-countdown {
    font-family: var(--font-mono);
    font-size: 0.85rem;
    color: var(--neon-orange);
    margin: 12px 0;
}

.debt-status {
    margin-top: 12px;
}

.debt-status label {
